    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/genTest.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "command": "yarn jest",
            "name": "Run test",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "yarn start",
            "name": "Start vite dev",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "yarn serve",
            "name": "Start serve",
            "request": "launch",
            "type": "node-terminal"
        },
    ]
}