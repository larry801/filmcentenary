{
  "include": [
    "./src/*"
  ],
  "compilerOptions": {
    "baseUrl": "./src/",
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "jsx": "react-jsx",
    "target": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "sourceMap": true,
    "types": [
      "vite/client",
    ],
    "noFallthroughCasesInSwitch": true
  }
}
